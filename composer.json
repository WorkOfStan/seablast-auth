{
  "name": "seablast/auth",
  "description": "No-password authentication and authorisation library for Seablast for PHP",
  "type": "library",
  "require": {
    "php": ">=7.2 <8.5",
    "guzzlehttp/guzzle": "^7.8",
    "nette/utils": "^3.2.10 || ^4.0.5",
    "robmorgan/phinx": "^0.12.13 || ^0.13.4 || ^0.14.0 || ^0.15.5 || ^0.16.5",
    "seablast/interfaces": "^0.1.1",
    "symfony/mailer": "^4.4.49 || ^5 || ^6 || ^7",
    "symfony/security-csrf": "^4.4.37 || ^5 || ^6 || ^7",
    "tracy/tracy": "^2.9.8 || ^2.10.9",
    "webmozart/assert": "^1.10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4 || ^9 || ^10 || ^11 || ^12",
    "seablast/seablast": "^0.2.5"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Stanislav Rejthar",
      "email": "rejthar@stanislavrejthar.com"
    }
  ],
  "support": {
    "email": "rejthar@stanislavrejthar.com",
    "issues": "https://github.com/WorkOfStan/seablast-auth/issues",
    "source": "https://github.com/WorkOfStan/seablast-auth"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Seablast\\Auth\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Seablast\\Auth\\Tests\\": [
        "tests/"
      ]
    }
  }
}
